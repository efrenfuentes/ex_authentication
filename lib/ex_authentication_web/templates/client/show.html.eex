<h2>Show Client</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @client.name %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @client.description %>
  </li>

  <li>
    <strong>Secret key:</strong>
    <%= @client.secret_key %>
  </li>

</ul>

<span><%= link "Edit", to: client_path(@conn, :edit, @client) %></span>
<span><%= link "Back", to: client_path(@conn, :index) %></span>
